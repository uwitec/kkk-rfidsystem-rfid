<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-autowire="byName">

	<bean id="statusServer" class="com.rfid.device.server.impl.StatusServerImpl" />
	
	<bean id="deviceServer" class="com.rfid.device.server.impl.DeviceServerImpl" />
	
	<bean id="readerServer" class="com.rfid.device.server.impl.ReaderServerImpl" />
	
	<bean id="areaServer" class="com.rfid.device.server.impl.AreaServerImpl" />
	
	<bean id="deviceManagerServer" class="com.rfid.device.server.impl.DeviceManagerServerImpl" />
	
	
</beans>